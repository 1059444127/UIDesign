                <EventTrigger RoutedEvent="FrameworkElement.Loaded">
                    <BeginStoryboard>
                        <Storyboard  AutoReverse="True" >
                            <ObjectAnimationUsingKeyFrames   Storyboard.TargetName="Ellipse0" Storyboard.TargetProperty="(UIElement.Visibility)">
